{
    "name": "dragon-code/translation-set",
    "description": "Translation set for The Dragon Code packages",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "languages",
        "lang",
        "translations",
        "messages",
        "dragon code"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@dragon-code.pro",
            "homepage": "https://github.com/andrey-helldar"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "laravel-lang/publisher": "^14.6"
    },
    "require-dev": {
        "laravel-lang/status-generator": "^1.13",
        "phpunit/phpunit": "^9.6",
        "symfony/var-dumper": "^6.2"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DragonCode\\TranslationSet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "YourNamespace\\Translations\\ServiceProvider"
            ]
        }
    }
}
